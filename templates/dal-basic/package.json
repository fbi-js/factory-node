  "license": "ISC",
  "dependencies": {
    "@mrapi/dal": "1.0.0",
    "@mrapi/cli": "1.0.0",
    "cross-env": "^7.0.2"
  },
  "devDependencies": {
    "prettier": "^2.1.1"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all"
  }
}