{
  "name": "<%= project.name %>",
  "version": "0.0.0",
  "description": "<%= project.description %>",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "fbi s",
    "prod": "cross-env NODE_ENV=production node dist/src/app",
    "build": "rm -rf dist && npm run generate && tsc",
    "clean": "rm -rf dist && rm -rf node_modules",
    "prettier": "prettier --write src/**/*.ts",
